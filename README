NAME
    METS::Parse - Class for METS parsing.

SYNOPSIS
     use METS::Parse;
     my $obj = METS::Parse->new;
     my $mets_hr = $obj->parse($mets_data);

METHODS
    "new()"
             Constructor.

    "parse($mets_data)"
             Parse XML data via XML::Simple::XMLin().
             Returns hash reference to structure.

ERRORS
     new():
             From Class::Utils::set_params():
                     Unknown parameter '%s'.

EXAMPLE
     # Pragmas.
     use strict;
     use warnings;

     # Modules.
     use Data::Printer;
     use METS::Parse;

     # Example METS data.
     my $mets_data <<'END';
     TODO
     END

     # Object.
     my $obj = METS::Parse->new;

     # Parse.
     my $mets_hr = $obj->parse($mets_data);

     # Dump to output.
     p $mets_hr;

     # Output like:
     # TODO

DEPENDENCIES
    Class::Utils, XML::Simple.

REPOSITORY
    <https://github.com/tupinek/METS-Parse>

